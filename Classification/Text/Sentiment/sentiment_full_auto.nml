oracle("mode") = "classification"
oracle("complexity") =  0.1 #try 0.5, 0.75, etc. for greater complexity


source:
    bind = "training_data.csv" ;
    input:
        Review -> text: [200];
    output:
        Label -> number: [2];
    params:
        validation_split = 0.5;

architecture:
    Review -> auto -> Label ;

train:
    compile:
        metrics = ['accuracy'] ;
    run:
        epochs = 20 ;
    dashboard: ;
